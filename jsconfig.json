{
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": false,
    "paths": {
      "@server": ["./server/index"],
      "@gql/*": ["./server/gql/*"],
      "@utils/*": ["./server/utils/*"],
      "@utils": ["./server/utils/index"],
      "@config/*": ["./server/config/*"],
      "@database/*": ["./server/database/*"],
      "@database": ["./server/database/index"]
    }
  }
}
